SELECT title, duration 
FROM tracks 
ORDER BY duration DESC 
LIMIT 1;

SELECT title, duration 
FROM tracks 
WHERE duration >= 210 
ORDER BY duration DESC;

SELECT title, release_year 
FROM compilations 
WHERE release_year BETWEEN 2018 AND 2020 
ORDER BY release_year;

SELECT name 
FROM artists 
WHERE name NOT LIKE '% %' 
AND name NOT LIKE '%-%';

SELECT title 
FROM tracks 
WHERE LOWER(title) LIKE '%мой%' 
OR LOWER(title) LIKE '%my%';
